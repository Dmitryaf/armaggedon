{
  "env": {
      "browser": true,
      "es6": true
    },
    "extends": [
      "airbnb"
    ],
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true
      },
      "sourceType": "module"
    },
    "plugins": [
      "react"
    ],
    "rules": {
      "linebreak-style": "off",
      "react/prop-types": "off"
    }
}